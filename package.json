  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "turbo run build --no-daemon",
    "dev": "turbo dev --no-daemon",
    "clean": "run-p clean:turbo clean:root",
    "clean:turbo": "turbo run clean --no-daemon",
    "clean:root": "rm -rf .turbo dist artifacts node_modules/.cache",
    "full-clean": "run-p full-clean:turbo full-clean:root",
    "full-clean:turbo": "turbo run full-clean --no-daemon",
    "full-clean:root": "rm -rf .turbo dist node_modules artifacts",
    "prisma:generate:api": "turbo run prisma:generate --filter=api --no-daemon",
    "prisma:migrate:api": "turbo run prisma:migrate --filter=api --no-daemon",
    "prisma:studio:api": "turbo run prisma:studio --filter=api --no-daemon",
    "prisma:pull:api": "turbo run prisma:pull --filter=api --no-daemon",
    "prisma:reset:api": "turbo run prisma:reset --filter=api --no-daemon"
  },
