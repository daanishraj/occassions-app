{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Default",
    "compilerOptions": {
      "target": "es2016",
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "allowJs": true,
      "sourceMap": true,
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "noUnusedLocals": false
    }
  }
  