{
  "name": "@occassions/types",
  "version": "0.0.1",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && node -e \"require('fs').cpSync('dist/esm/index.d.ts','dist/index.d.ts',{force:true}); require('fs').writeFileSync('dist/index.js', 'export * from \\\"./esm/index.js\\\"\\n');\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "zod": "^3.23.4"
  }
}
